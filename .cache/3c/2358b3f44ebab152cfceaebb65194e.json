n      <img\n        {...props.attributes}\n        src={src}\n        style={{ width: \"100%\", height: \"auto\" }}\n      />\n    );\n  }\n};\n\nfunction renderBlock(props, editor, next) {\n  const { node } = props;\n  return BLOCKS[node.type] ? BLOCKS[node.type](props) : next();\n}\n\nconst TextEditor = ({ onTitleChange, title, ...props }) => {\n  const editorRef = useRef(null);\n\n  const onTitleKeyPress = useCallback(e => {\n    if (e.key === \"Tab\" || e.key === \"Enter\") {\n      console.log(editorRef.current);\n      editorRef.current.focus();\n    }\n  });\n\n  return (\n    <>\n      <Title\n        value={title}\n        onChange={e => {\n          onTitleChange(e.target.value);\n        }}\n        onKeyPress={onTitleKeyPress}\n      />\n      <EditorBar\n        onAddImage={() => {\n          const src = \"https://i.imgur.com/Onbcgek.jpg\";\n          editorRef.current.insertBlock({\n            type: \"image\",\n            data: {\n              src\n            },\n            key: \"image-node-1\"\n          });\n        }}\n      />\n      <StyledEditor ref={editorRef} {...props} renderBlock={renderBlock} />\n    </>\n  );\n};\n\nexport default TextEditor;\n"},"lineCount":null}},"error":null,"hash":"056774b7d73e991c595ace5c8bfa0d1b","cacheData":{"env":{}}}