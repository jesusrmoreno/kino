Items: \"center\" }}\n      >\n        <img style={{ height: 14, width: 14 }} src={PlusIcon} />\n      </div>\n      <span className=\"h6\" style={{}}>\n        ADD BLOCK\n      </span>\n    </div>\n  );\n};\n\nconst Editorbar = ({ onAddImage }) => {\n  return (\n    <Container>\n      <Icon src={PictureIcon} onClick={onAddImage} />\n      <Icon src={VideoPlayerIcon} />\n      <Icon src={AttachmentIcon} />\n      <Icon src={MapIcon} />\n      <Icon src={LinkIcon} />\n    </Container>\n  );\n};\n\nexport default Editorbar;\n"},"lineCount":null}},"error":null,"hash":"fcf1ff623098794fbec474f9d915e183","cacheData":{"env":{}}}