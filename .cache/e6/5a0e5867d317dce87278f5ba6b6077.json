e","generated":{"line":60,"column":15},"source":"src/components/Editor/useAutoSave.js","original":{"line":58,"column":15}}],"sources":{"src/components/Editor/useAutoSave.js":"import { useEffect, useState, useRef } from \"react\";\nimport { saveDocument } from \"api/document\";\n\nimport moment from \"moment\";\n\nconst useAutoSave = (_id, value, title) => {\n  const [{ draftIsSaving, draftLastSaved }, setState] = useState({\n    draftLastSaved: null,\n    draftIsSaving: false\n  });\n\n  const lastValue = useRef(null);\n  const lastTitle = useRef(null);\n\n  const documentId = useRef(_id);\n\n  useEffect(() => {\n    documentId.current = _id;\n  }, [_id]);\n\n  useEffect(() => {\n    if (!draftIsSaving) {\n      if (value) {\n        if (\n          value.document !== lastValue.current ||\n          title !== lastTitle.current\n        ) {\n          lastValue.current = value.document;\n          lastTitle.current = title;\n\n          const id = setTimeout(async () => {\n            setState(s => ({\n              ...s,\n              draftIsSaving: true\n            }));\n            const res = await saveDocument({\n              _id: documentId.current,\n              content: JSON.stringify(value.toJSON()),\n              title,\n              text: \"\"\n            });\n\n            documentId.current = res._id;\n            setState({\n              draftIsSaving: false,\n              draftLastSaved: moment()\n            });\n          }, 250);\n          return () => clearTimeout(id);\n        }\n      }\n    }\n  });\n\n  return [draftLastSaved, draftIsSaving];\n};\n\nexport default useAutoSave;\n"},"lineCount":null}},"error":null,"hash":"7c04b1ba1c77b7bba4e6311b300dcbf8","cacheData":{"env":{}}}